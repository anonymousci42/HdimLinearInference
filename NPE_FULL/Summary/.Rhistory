beta.true = c(1,1,0,0,0,rep(0,p-5))
index.ini = 1:p
i=5
repeatnum = 100
record.pvalue = record.stat = rep(0, repeatnum)
for (m in 1:repeatnum) {
X = mvrnorm(n = n, mu = mu, Sigma = A)
error <- rnorm(n = n,0,sigma)
Y = X%*%beta.true + 1*error
Y = as.vector(Y)
### Decorrelated score method
index = index.ini[-i]
Z = X[,index]
Z1 = cbind(1,Z)
Xi = X[,i]
cvfit1 = cv.ncvreg(Z,Xi,penalty='lasso',nfolds = 5)
cvfit2 = cv.ncvreg(Z,Y,penalty='lasso',nfolds = 5)
model1 = cvfit1$fit
model2 = cvfit2$fit
hattheta = as.numeric(model1$beta[,cvfit1$min])
res = Xi - Z1%*%hattheta
fisher = t(Xi)%*%Xi/n-t(hattheta)%*%(apply(Z1*Xi,2,mean))
hatgammak = as.numeric(model2$beta[,cvfit2$min])
res1 = Y-Z1%*%hatgammak
sigmahat = t(res1)%*%res1/n
eta = res1*res
Un = sum(eta)/sqrt(sigmahat*n*fisher)
record.pvalue[m] = 2*(1-pnorm(abs(Un)))#ifelse(abs(Un)>=qnorm(0.975),1,0)
record.stat[m]= Un
print(c(m,record.pvalue[m]))
}
